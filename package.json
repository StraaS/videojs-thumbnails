{
  "name": "videojs-thumbnails",
  "description": "progress bar thumbnails plugin for video.js",
  "browserslist": [
    "> 1%",
    "not dead"
  ],
  "eslintConfig": {
    "extends": "airbnb-base",
    "parser": "babel-eslint",
    "env": {
      "browser": true,
      "es6": true
    },
    "globals": {
      "videojs": true
    },
    "rules": {
      "semi": [
        "error",
        "never"
      ],
      "global-require": [
        "off"
      ],
      "implicit-arrow-linebreak": [
        "off",
        "beside"
      ],
      "brace-style": [
        "error",
        "stroustrup"
      ],
      "no-bitwise": [
        "off"
      ],
      "no-plusplus": [
        "off"
      ],
      "no-param-reassign": [
        "off"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:brightcove/videojs-thumbnails.git"
  },
  "scripts": {
    "build": "./node_modules/.bin/rollup -c --environment BUILD:production",
    "dev": "./node_modules/.bin/rollup -c -w --environment BUILD:development"
  },
  "version": "0.1.1",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "babel-eslint": "^10.0.1",
    "babel-plugin-lodash": "^3.3.4",
    "eslint": "^5.3.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^1.1.0",
    "rollup-plugin-babel": "^4.3.0",
    "rollup-plugin-css-only": "^0.4.0",
    "rollup-plugin-eslint": "^5.0.0",
    "rollup-plugin-livereload": "^0.6.0",
    "rollup-plugin-serve": "^1.0.0",
    "video.js": "^4.12",
    "videojs-generate-rollup-config": "^2.3.2"
  },
  "dependencies": {
    "lodash": "^4.17.11"
  }
}
