{
  "name": "videojs-thumbnails",
  "description": "progress bar thumbnails plugin for video.js",
  "browserslist": [
    "> 1%",
    "not dead"
  ],
  "eslintConfig": {
    "extends": "airbnb-base",
    "parser": "babel-eslint",
    "env": {
      "browser": true,
      "es6": true
    },
    "globals": {
      "videojs": true
    },
    "rules": {
      "semi": [
        "error",
        "never"
      ],
      "global-require": [
        "off"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:brightcove/videojs-thumbnails.git"
  },
  "scripts": {
    "build": "grunt build",
    "dev": "grunt dev"
  },
  "version": "0.1.1",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "babel-eslint": "^10.0.1",
    "babel-plugin-iife-wrap": "^1.1.0",
    "eslint": "^5.3.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "grunt": "^0.4.4",
    "grunt-babel": "^8.0.0",
    "grunt-cli": "^1.3.2",
    "grunt-contrib-connect": "^0.9.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-watch": "^1.1.0",
    "grunt-eslint": "^21.0.0",
    "load-grunt-tasks": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "video.js": "^4.12"
  },
  "dependencies": {}
}
