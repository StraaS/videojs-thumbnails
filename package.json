{
  "name": "@straas/videojs-thumbnails",
  "description": "progress bar thumbnails plugin for video.js",
  "browserslist": [
    "> 1%",
    "not dead"
  ],
  "main": "./dist/videojs-thumbnails.cjs.js",
  "module": "./dist/videojs-thumbnails.es.js",
  "style": "./dist/videojs-thumbnails.css",
  "eslintConfig": {
    "extends": "airbnb-base",
    "parser": "babel-eslint",
    "env": {
      "browser": true,
      "es6": true
    },
    "globals": {
      "videojs": true,
      "QUnit": true
    },
    "rules": {
      "semi": [
        "error",
        "never"
      ],
      "global-require": [
        "off"
      ],
      "implicit-arrow-linebreak": [
        "off",
        "beside"
      ],
      "brace-style": [
        "error",
        "stroustrup"
      ],
      "no-bitwise": [
        "off"
      ],
      "no-plusplus": [
        "off"
      ],
      "no-param-reassign": [
        "off"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:StraaS/videojs-thumbnails.git"
  },
  "scripts": {
    "build": "./node_modules/.bin/rollup -c --environment BUILD:production",
    "dev": "./node_modules/.bin/rollup -c -w --environment BUILD:development"
  },
  "version": "0.2.2",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "babel-eslint": "^10.0.1",
    "babel-plugin-lodash": "^3.3.4",
    "eslint": "^5.3.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "karma": "^3.1.4",
    "karma-chrome-launcher": "^2.2.0",
    "karma-firefox-launcher": "^1.1.0",
    "karma-qunit": "^2.1.0",
    "karma-rollup-preprocessor": "^7.0.0-rc.2",
    "npm-run-all": "^4.1.5",
    "qunit": "^2.9.1",
    "rollup": "^1.1.0",
    "rollup-plugin-babel": "^4.3.0",
    "rollup-plugin-css-only": "^0.4.0",
    "rollup-plugin-eslint": "^5.0.0",
    "rollup-plugin-livereload": "^0.6.0",
    "rollup-plugin-serve": "^1.0.0",
    "standard-version": "^4.4.0",
    "videojs-generate-rollup-config": "^2.3.2"
  },
  "dependencies": {
    "lodash": "^4.17.11"
  },
  "peerDependencies": {
    "video.js": "^7.4.1"
  }
}
